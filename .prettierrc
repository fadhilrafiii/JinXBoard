{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^Redux/Store",
    "^Redux/Reducers",
    "^Redux/Actions",
    "^Pages",
    "^Layout",
    "^Components",
    "^Shared/[Types|Enums]",
    "^Shared/Helpers",
    "^Shared/Constants",
    "^[../](.*)(?<!.css)$",
    "^[./](.*)(?<!.css)$",
    "(.*)css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
